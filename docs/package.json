	"description": "project to test ag-grid component",
	"main": "index.js",
	"private": true,
	"scripts": {
		"start": "npx gulp default",
		"compile:all": "gulp compile:all",
		"watch:all": "npx gulp watch:all",
		"server-only": "npx gulp server",
		"test:js": "npx jest",
		"fix": "npm run fix:code && npm run fix:styles && npm run fix:html",
		"fix:code": "npx prettier --write src/**/*.js",
		"fix:styles": "npx prettier --write src/**/*.css",
		"fix:html": "npx  prettier --write src/**/*.{html,htm} ",
		"check:eslint": "eslint --print-config . | eslint-config-prettier-check",
		"check:stylelint": "stylelint-config-prettier-check",
		"gen:docs": "npx gulp gen:docs",
		"preinstall": "node config/install/install.js",
		"debug": "node  --nolazy --inspect-brk=9300 config/install/install.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://gitlab.com/lcsfwd/default-boilerplate.git"
	},
